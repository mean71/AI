   "source": [
    "# 유클리드 호제법 반복문\n",
    "def Euclidean(a, b):\n",
    "    while b != 0:\n",
    "        [a, b] = [b, a%b]\n",
    "    return a"
   ]
  },
  {
   "cell_type": "code",
